{
  "manifest_version": 2,
  "name": "BlackBean",
  "version": "0.1",
 "background": {
    "scripts": ["background.js"],
	"persistent": true
  },
  "permissions": ["alarms", "tabs"],
"content_scripts": [
  {
	"matches": [
	  "<all_urls>"
	],
	 "js": ["jquery-3.2.1.min.js", "content.js"]
  }
],
 "browser_action": {
    "default_icon": "icon.png",
	"default_title": "BlackBean man"
  }
}
